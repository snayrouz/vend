<h1><%= @machine.location %> Vending Machine</h1>

<table>
  <thead>
    <th>Snack</th>
    <th>Price</th>
  </thead>
  <tbody>
    <tr><% @snacks.each do |snack|  %></tr>
      <td> <%= snack.name %></td>
      <td> <%= snack.price.to_f %></td>
      <p>Average Price: $<%= snack.average_price %></p>
      <% end %>
  </tbody>
</table>
